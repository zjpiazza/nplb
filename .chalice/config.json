{
  "version": "2.0",
  "app_name": "nplb",
  "stages": {
    "dev": {
      "api_gateway_stage": "api",
      "environment_variables": {
        "AWS_ENDPOINT_URL": "http://localhost:4566"
      },
      "resources": [
        {
          "name": "builds-queue",
          "resource_type": "sqs",
          "queue_name": "builds"
        }
      ],
      "events": [
        {
          "source": "builds-queue",
          "name": "builds-queue-event",
          "handler": "app.build_handler"
        }
      ]
    }
  }
}